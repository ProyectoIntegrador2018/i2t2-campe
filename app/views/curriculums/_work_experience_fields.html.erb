<div class="card-panel">
  <div class='nested-fields'>
    <div class="row">
      <div class="col s12">
        <%= f.label :Empresa %>
        <%= f.text_field :company %>
      </div>
    </div>
    
    <div class="row">
      <div class="col s12 l6">
        <%= f.label :Entidad %>
        <%= f.text_field :entity %>
      </div>
      <div class="col s12 l6">
        <%= f.label :País %>
        <%= f.text_field :country %>
      </div>
    </div>

    <div class="row">
      <div class="col s12">
        <%= f.label :Posición %>
        <%= f.text_field :title %>
      </div>
    </div>

    <div class="row">
      <div class="col s12">
        <%= f.label :Área_del_conocimiento %>
        <%= f.text_field :study_area %>
      </div>
    </div>

    <div class="row">
      <div class="col s12 l6">
        <%= f.label :Fecha_de_inicio %>
        <%= f.date_field :start_date %>
      </div>
      <div class="col s12 l6">
        <%= f.label :Fecha_de_fin %>
        <%= f.date_field :end_date %>
      </div>
    </div>

    <div class="row">
      <div class="col s12">
        <%= f.label :Descripcion %>
        <%= f.text_area :description, class: 'materialize-textarea' %>
      </div>
    </div>
    <div class="row">
      <div class="col s1 offset-s11">
        <%= link_to_remove_association f, :class => "btn-floating red" do %>
          <i class="large material-icons">delete</i>
        <% end %>
      </div>
    </div>
    
  </div>
</div>